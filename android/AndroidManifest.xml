<?xml version='1.0' encoding='utf-8'?>
<manifest package="org.kflog.cumulus8" android:versionCode="1" android:installLocation="auto" xmlns:android="http://schemas.android.com/apk/res/android" android:versionName="2.12.2_Froyo">
