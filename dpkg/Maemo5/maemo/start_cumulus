#!/bin/sh
# $Id$
# Maemo start script for cumulus. The own Qt libraries have to be taken into account

# set qt library path for run-time loader
LD_LIBRARY_PATH=/opt/cumulus/lib
export LD_LIBRARY_PATH

# workaround to start browser from QTextView
BROWSER="browser --url"
export BROWSER

# reset environment variable to prevent ui modifications defined in /etc/osso-af-init/osso-gtk.defs
GTK2_RC_FILES=""
export GTK2_RC_FILES

# launch cumulus
exec /opt/cumulus/bin/cumulus -style=platinum


